{
  "name": "electrobun-react-tailwind-vite",
  "version": "1.0.0",
  "description": "Electrobun app with React, Tailwind CSS, and Vite HMR",
  "scripts": {
    "dev": "bun run build && electrobun dev",
    "dev:hmr": "concurrently \"bun run hmr\" \"bun run dev\"",
    "hmr": "vite --port 5173",
    "build": "vite build && electrobun build",
    "build:prod": "vite build && electrobun build --channel prod",
    "start": "bun run dev",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:check": "drizzle-kit check --config=drizzle.config.ts",
    "db:studio": "bun run src/bun/db/cli/studio.ts",
    "format": "oxfmt --ignore-path .oxfmtignore --write .",
    "format:check": "oxfmt --ignore-path .oxfmtignore --check .",
    "lint": "oxlint .",
    "lint:fix": "oxlint --fix ."
  },
  "dependencies": {
    "@base-ui/react": "^1.2.0",
    "@tanstack/react-router": "^1.161.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "electrobun": "1.12.3",
    "lucide-react": "^0.574.0",
    "react": "^19.2.4",
    "react-day-picker": "^9.13.2",
    "react-dom": "^19.2.4",
    "smol-toml": "^1.6.0",
    "tailwind-merge": "^3.4.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/router-plugin": "^1.161.1",
    "@types/bun": "1.3.9",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^9.1.0",
    "drizzle-kit": "^0.31.9",
    "oxfmt": "^0.33.0",
    "oxlint": "^1.48.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
